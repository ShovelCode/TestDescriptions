#include "graphics.h"
#include <stdio.h>
#include <gb/gb.h> //8-bit gameboy programming library from a dev named loirak,
      //the main method below shows it being used


void main()
{
        printf("Welcome to GAMEBOY\nProgramming");
        printf("\nPress Start");
        waitpad(J_START);  // other keys are J_A, J_UP, J_SELECT, etc.
        printf("\nIsn't it easy!");
}
